{
  "name": "chainocracy-backend-tests",
  "version": "1.0.0",
  "description": "Comprehensive test suite for Chainocracy backend",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "build/**/*.js",
      "!build/tests/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "crypto-js": "^4.1.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "qrcode": "^1.5.3",
    "speakeasy": "^2.0.0",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "axios": "^1.9.0",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.5.0",
    "jest": "^29.5.0",
    "level": "^10.0.0",
    "nodemailer": "^7.0.3"
  }
}
