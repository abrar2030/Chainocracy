{
  "name": "chainocracy-backend-api",
  "version": "1.0.0",
  "description": "Backend API for Chainocracy project",
  "main": "build/api/index.js",
  "scripts": {
    "dev": "ts-node-dev src/network.ts",
    "build": "rimraf ./build && tsc",
    "start": "node build/api/index.js",
    "test": "jest",
    "lint": "npx eslint .",
    "node_0": "npx nodemon src/network.ts",
    "node_1": "nodemon --watch dev -e js dev/networkNode.js 3001 http://localhost:3001",
    "node_2": "nodemon --watch dev -e js dev/networkNode.js 3002 http://localhost:3002"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "level": "^8.0.1",
    "nodemailer": "^6.9.13",
    "qrcode": "^1.5.3",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "babel-jest": "^29.5.0",
    "eslint": "^8.42.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "run-parallel": "^1.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/*.test.js", "**/build/tests/**/*.test.js"]
  },
  "license": "ISC"
}
